  },
  "keywords": [
    "Sails",
    "CRUD",
    "Admin"
  ],
  "author": "Victor Trejo",
  "license": "MIT",
  "sails": {
    "isHook": true
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-3": "^6.24.1",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "ignore-loader": "^0.1.2",
    "node-sass": "^4.9.0",
    "sails": "~0.11.0",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^3.8.1",
    "webpack-node-externals": "^1.2.0"
  },
  "dependencies": {
    "react": "^16.4.0",
    "react-crud-table": "0.0.2",
    "react-dom": "^16.4.0",
    "react-router-dom": "^4.2.2",
    "sass-loader": "^7.0.1",
    "semantic-ui-css": "^2.3.1",
    "semantic-ui-react": "^0.80.2",
    "simple-json-requester": "0.0.4",
    "simple-json-table": "^1.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/config/assetsTransformer.js"
    }
  }
}
